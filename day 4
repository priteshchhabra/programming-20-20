def inptSuduko():
    row = []
    print("enter 0 if you don't want to fill a space in the row:")
    for i in range(9):
        r = int(input("enter the numbers in the 1st row:"))
        row.append(r)
    return (row)
s = inptSuduko()
print(s)

def checkRow(s):
    row = []
    for i in range(len(s)):
        flag = 0
        if 0 not in s:
            x = s[i]
            del s[i]
            if x in s:
                flag = 0
                exit
            else:
                flag = 1
        else:
            x = s[i]
            del s[i]
            if x in s:
                flag = 2
                exit
            else:
                flag = 1
        return (flag)
r = checkRow(s)


def printSuduko(s):
    rw = []
    for i in range(8):
        x = s
        rw.append(x)
    return rw
y = printSuduko(s)
print(y)    

if r == 0:
    print("inviable")
elif r == 1:
    print("viable")
else:
    print("viable and not complete")
    
