class DNA:
    def __init__(self,nucleotides):
        self.nucleotides=nucleotides
class all_DNA:
    def __init__(self):
        self.dna=[]
    def add_dna(self,n):
        self.dna.append(n)
class substring:
    def __init__(self,longest,distinct):
        self.longest=longest
        self.distinct=distinct
class substrins:
    def __init__(self):
        self.longests=[]
        self.distincts=[]

    def add_substring(self,l,d):
        self.longests.append(l)
        self.distinct.append(d)
