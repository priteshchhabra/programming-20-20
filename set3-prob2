class UI:
    def input(self):
        n = int(input("enter number of test cases :"))
        d = all_DNA()
        for i in range(n):
            x = input("enter the string: ")
            w = DNA(x)
            d.add_dna(w)
        return d
    def ouput(self,imprt):
        for i in range():
